.row
  .col-md-12
    .panel.panel-default.header-part
      .panel-heading
        .row
          .col-md-3
            %h3.panel-title
              %i.fa.fa-list
              Grants
          .col-md-9
            = render 'index_actions'
      .panel-body
        - if @grants.empty?
          %p No matching grants found
        - else
          .col-md-12
            .chart{:id => "chart_#{@report.id}"}
          .col-md-12                
            = render 'index_table'

= render 'index_scripts'
